{
  "name": "life-style",
  "version": "1.0.0",
  "description": "Lifestyle",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/C4co/lifestyle.git"
  },
  "main": "index.js",
  "scripts": {
    "start": "parcel index.html --port=3000",
    "build": "parcel build index.html --out-dir dist --public-url ./ && workbox generate:sw && cp _redirects dist",
    "csscomb": "csscomb ./src/*.scss",
    "test": "jest --watch --coverage --verbose",
    "test:once": "jest --coverage --verbose",
    "test:e2e": "codeceptjs run --steps",
    "new:page": "node gen/run.page.js",
    "standard": "standard 'src/*.js' --fix"
  },
  "alias": {
    "react": "preact-compat",
    "react-dom": "preact-compat"
  },
  "standard": {
    "globals": [
      "Feature",
      "Scenario",
      "it",
      "angular",
      "module",
      "beforeEach",
      "Swiper",
      "describe",
      "expect"
    ],
    "ignore": [
      "/dist",
      "/assets"
    ]
  },
  "jest": {
    "coverageDirectory": ".coverage/",
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/src"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "ts",
      "tsx",
      "css",
      "scss",
      "less"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy",
      "^react$": "preact-compat",
      "^react-dom$": "preact-compat"
    },
    "testRegex": "(/__tests__/.*\\.(test|spec))\\.(js)$"
  },
  "author": "Carlos costa - C4co",
  "license": "MIT",
  "devDependencies": {
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-preact": "^1.1.0",
    "chalk": "^2.3.2",
    "codeceptjs": "^1.1.6",
    "codeceptjs-puppeteer": "^1.1.0",
    "csscomb": "^4.2.0",
    "dotenv": "^5.0.1",
    "highlight.js": "^9.12.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.2",
    "node-sass": "^4.7.2",
    "parcel-bundler": "^1.6.2",
    "preact-compat": "^3.18.0",
    "preact-render-to-string": "^3.7.0",
    "puppeteer": "^1.1.1",
    "shelljs": "^0.8.1",
    "standard": "^11.0.0",
    "styled-components": "^3.2.0"
  },
  "dependencies": {
    "preact": "^8.2.7",
    "preact-router": "^2.6.0",
    "whatwg-fetch": "^2.0.3"
  },
  "bugs": {
    "url": "https://github.com/C4co/lifestyle/issues"
  },
  "homepage": "https://github.com/C4co/lifestyle#readme"
}
